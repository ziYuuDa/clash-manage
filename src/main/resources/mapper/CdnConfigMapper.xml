<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="clash.manage.dao.CdnConfigMapper">

    <select id="list" resultType="clash.manage.model.CdnConfig">
        SELECT *
        FROM cdn_config
    </select>
    <select id="selectByName" resultType="clash.manage.model.CdnConfig">
        SELECT *
        FROM cdn_config
        WHERE name = #{name}
    </select>

</mapper>